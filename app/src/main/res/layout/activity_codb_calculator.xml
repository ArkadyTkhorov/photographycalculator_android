<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="info.androidhive.materialtabs.fragments.CODBCalculator"
    android:background="#cccccc">

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="COST OF DOING BUSINESS / STUDIO OVERHEAD CALCULATOR"
        android:textColor="#ff000000"
        android:textSize="12sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(all figures are annually)"
        android:textColor="#ff000000"
        android:textSize="12sp"
        android:textStyle="bold"
        android:layout_below="@+id/title" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/title"
        android:layout_marginTop="25dp" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/title"
                android:text="Office/Studio Rent"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"
                android:id="@+id/officerent" />

            <EditText
                android:id="@+id/etxtOffice"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txtOffice"
                android:text="Phone/Cell Cost"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtPhone"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/title"
                android:text="Average Equipment Purchases"
                android:id="@+id/txtwtf"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtPhoto"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/title"
                android:text="Equipment Service/Repairs"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtEquipment"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/title"
                android:text="Computer Costs (Hardware + Software)"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtComputer"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/title"
                android:text="Internet Cost"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtBroad"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/title"
                android:text="Web Hosting Cost"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtWebhostin"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/title"
                android:text="Vehicle Expenses / Maintenance"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtVehicle"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Office Supplies"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtOfficeSupplies"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Postage and Shipping"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtPostage"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:text="Professional Fees"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtProfessional"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Advertising and Promotion"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtAdvertising"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Subscriptions and Dues"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtSubscriptions"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Insurance (Equipment + Business)"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtEquipmentBusiness1"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Health Insurance Fees"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtHealth"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Legal and Accounting Services"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtLegal"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Taxes and Licenses"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtTaxes"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Office Assistant"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtOfficeAssistance"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Utilities (Electricity/Gas/Water)"
                android:textSize="11sp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"/>

            <EditText
                android:id="@+id/etxtUtilities"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/title"
                android:text="Travel and Entertainment"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/etxtTravel"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_below="@+id/title"
                android:text="INCOME FACTORS"
                android:textColor="@android:color/black"
                android:textSize="15sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/title"
                android:text="Entire your desired annual salary:"
                android:layout_marginBottom="10dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/etxtDesired"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/title"
                android:textColor="@android:color/black"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:text="Enter any non-photgraphy income:"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/etxtNonassignment"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:text="Enter number of billable business days per year:"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/etxtNumberofdays"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:inputType="numberDecimal|numberSigned"
                android:text="0" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/defaultValueButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="OnDefaultValue"
                    android:background="@color/orange"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:padding="10dp"
                    android:textColor="@android:color/white"
                    android:text="Use Default" />

                <Button
                    android:id="@+id/calculateButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@color/orange"
                    android:textColor="@android:color/white"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:text="Calculate" />
            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:textColor="@android:color/black"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:textStyle="bold"
                android:text="YOUR RESULTS" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Total Annual Expenses"
                android:textSize="15sp"
                android:textColor="@android:color/black"/>

            <TextView
                android:id="@+id/txtTotalAnnualExpenses"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000"
                android:textSize="24sp"
                android:text="" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Your weekly cost of doing business:"
                android:textColor="@android:color/black"
                android:textSize="15sp"/>

            <TextView
                android:id="@+id/txtWeeklyCost"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000"
                android:textSize="24sp"
                android:text="" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Your overheard cost for each billable business day:"
                android:textSize="15sp"
                android:textColor="@android:color/black"/>

            <TextView
                android:id="@+id/txtOverheadCost"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000"
                android:textSize="24sp"
                android:layout_below="@+id/title"
                android:text="" />
        </LinearLayout>
    </ScrollView>

</RelativeLayout>
